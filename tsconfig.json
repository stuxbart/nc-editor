{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "target": "es6",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "strict": true,
    "module": "commonjs",
    "declaration": true,
    "newLine": "lf",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
  },
  "include": ["src"],
  "exclude": ["example", "public"]
}
